<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- BUTTONS -->
    <declare-styleable name="DSTicketInfo">
        <attr name="ds_info_type" />
        <attr name="ds_rounded_bottom" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DSTicketPrice">
        <attr name="price" format="float"/>
        <attr name="tax" format="float"/>
        <attr name="singleLine" format="boolean"/>
        <attr name="selected"/>
    </declare-styleable>

    <attr name="ds_info_type" format="enum">
        <enum name="description" value="0" />
        <enum name="max" value="1" />
        <enum name="min" value="2" />
        <enum name="passkey" value="3" />
    </attr>
</resources>